<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.destinyapp.aplikasipanduanibuhamil">

    <application
        android:allowBackup="true"
        android:icon="@drawable/unnamed"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.DetailInformasiActivity"
            android:label="Informasi Kehamilan"
            android:parentActivityName=".Activity.InformasiKehamilanActivity"></activity>
        <activity
            android:name=".Activity.MitosActivity"
            android:label="Mitos Kehamilan"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.TentangActivity"
            android:label="Tentang Saya"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.KalkulatorHPLActivity"
            android:label="Kalkulator Hari Perkiraan Lahir"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.InformasiKehamilanActivity"
            android:label="Informasi Kehamilan"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/AppThemeNoAction" />
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>